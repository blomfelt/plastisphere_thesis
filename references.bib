@article{donoriodemeo2012MitoZoa20Database,
  title = {{{MitoZoa}} 2.0: A Database Resource and Search Tools for Comparative and Evolutionary Analyses of Mitochondrial Genomes in {{Metazoa}}},
  shorttitle = {{{MitoZoa}} 2.0},
  author = {{D'Onorio de Meo}, Paolo and D'Antonio, Mattia and Griggio, Francesca and Lupi, Renato and Borsani, Massimiliano and Pavesi, Giulio and Castrignan{\`o}, Tiziana and Pesole, Graziano and Gissi, Carmela},
  year = {2012},
  month = jan,
  journal = {Nucleic Acids Research},
  volume = {40},
  number = {Database issue},
  pages = {D1168-1172},
  issn = {1362-4962},
  doi = {10.1093/nar/gkr1144},
  abstract = {The MITOchondrial genome database of metaZOAns (MitoZoa) is a public resource for comparative analyses of metazoan mitochondrial genomes (mtDNA) at both the sequence and genomic organizational levels. The main characteristics of the MitoZoa database are the careful revision of mtDNA entry annotations and the possibility of retrieving gene order and non-coding region (NCR) data in appropriate formats. The MitoZoa retrieval system enables basic and complex queries at various taxonomic levels using different search menus. MitoZoa 2.0 has been enhanced in several aspects, including: a re-annotation pipeline to check the correctness of protein-coding gene predictions; a standardized annotation of introns and of precursor ORFs whose functionality is post-transcriptionally recovered by RNA editing or programmed translational frameshifting; updates of taxon-related fields and a BLAST sequence similarity search tool. Database novelties and the definition of standard mtDNA annotation rules, together with the user-friendly retrieval system and the BLAST service, make MitoZoa a valuable resource for comparative and evolutionary analyses as well as a reference database to assist in the annotation of novel mtDNA sequences. MitoZoa is freely accessible at http://www.caspur.it/mitozoa.},
  langid = {english},
  pmcid = {PMC3245153},
  pmid = {22123747},
  keywords = {Databases Nucleic Acid,DNA Mitochondrial,Evolution Molecular,Frameshifting Ribosomal,Genes Mitochondrial,Genome Mitochondrial,Introns,Mitochondrial Proteins,Molecular Sequence Annotation,Software},
  file = {/home/felix/Zotero/storage/AXCARFUF/D'Onorio de Meo et al. - 2012 - MitoZoa 2.0 a database resource and search tools for comparative and evolutionary analyses of mitoc.pdf}
}

