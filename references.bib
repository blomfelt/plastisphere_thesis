@article{2025WilcoxonSignedrankTest,
  title = {Wilcoxon Signed-Rank Test},
  year = {2025},
  month = may,
  journal = {Wikipedia},
  url = {https://en.wikipedia.org/w/index.php?title=Wilcoxon_signed-rank_test&oldid=1291114696},
  urldate = {2025-05-20},
  abstract = {The Wilcoxon signed-rank test is a non-parametric rank test for statistical hypothesis testing used either to test the location of a population based on a sample of data, or to compare the locations of two populations using two matched samples. The one-sample version serves a purpose similar to that of the one-sample Student's t-test. For two matched samples, it is a paired difference test like the paired Student's t-test (also known as the "t-test for matched pairs" or "t-test for dependent samples"). The Wilcoxon test is a good alternative to the  t-test when the normal distribution of the differences between paired individuals cannot be assumed. Instead, it assumes a weaker hypothesis that the distribution of this difference   is symmetric  around a central value  and it aims to test whether this center value differs significantly from zero. The Wilcoxon test is a more powerful alternative to the sign test because it considers the magnitude of the differences, but it requires this moderately strong assumption of symmetry.},
  copyright = {Creative Commons Attribution-ShareAlike License},
  langid = {english},
  annotation = {Page Version ID: 1291114696},
  file = {/home/felix/Zotero/storage/3GR3ZAYW/Wilcoxon_signed-rank_test.html}
}

@article{abramova2024MetagenomicAssembliesTend,
  title = {Metagenomic Assemblies Tend to Break around Antibiotic Resistance Genes},
  author = {Abramova, Anna and Karkman, Antti and {Bengtsson-Palme}, Johan},
  year = {2024},
  month = oct,
  journal = {BMC Genomics},
  volume = {25},
  number = {1},
  pages = {959},
  issn = {1471-2164},
  doi = {10.1186/s12864-024-10876-0},
  url = {https://doi.org/10.1186/s12864-024-10876-0},
  urldate = {2025-03-05},
  abstract = {Assembly of metagenomic samples can provide essential information about the mobility potential and taxonomic origin of antibiotic resistance genes (ARGs) and inform interventions to prevent further spread of resistant bacteria. However, similar to other conserved regions, such as ribosomal RNA genes and mobile genetic elements, almost identical ARGs typically occur in multiple genomic contexts across different species, representing a considerable challenge for the assembly process. Usually, this results in many fragmented contigs of unclear origin, complicating the risk assessment of ARG detections. To systematically investigate the impact of this issue on detection, quantification and contextualization of ARGs, we evaluated the performance of different assembly approaches, including genomic-, metagenomic- and transcriptomic-specialized assemblers. We quantified recovery and accuracy rates of each tool for ARGs both from in silico spiked metagenomic samples as well as real samples sequenced using both long- and short-read sequencing technologies.},
  keywords = {Antibiotic resistance genes,Genomic context,Metagenomic assembly},
  file = {/home/felix/Zotero/storage/24XVQMSN/Abramova et al. - 2024 - Metagenomic assemblies tend to break around antibiotic resistance genes.pdf;/home/felix/Zotero/storage/D77N6L2T/s12864-024-10876-0.html}
}

